<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net8.0;net9.0;net10.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageId>CSoft.$(AssemblyName)</PackageId>
		<Version>10.0.1</Version>
		<Company>CSoft</Company>
		<Authors>CSoft</Authors>
		<Description>单位转换拓展</Description>
		<PackageIcon>logo.png</PackageIcon>
		<PackageProjectUrl>https://github.com/caoyiliang/UnitExtend</PackageProjectUrl>
		<RepositoryUrl>https://github.com/caoyiliang/UnitExtend</RepositoryUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageReleaseNotes>$self $mol去除前面的$</PackageReleaseNotes>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\logo.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Flee" Version="2.0.0" />
	</ItemGroup>

</Project>
